{
  "author": {
    "email": "support@xylabs.com",
    "name": "XY Labs Development Team",
    "url": "https://xylabs.com"
  },
  "bin": {
    "build": "bin/build.js",
    "compile": "bin/compile.js",
    "xylabs-build": "bin/build.js",
    "xylabs-compile": "bin/compile.js"
  },
  "bugs": {
    "email": "support@xylabs.com",
    "url": "https://github.com/xylabs/ts-scripts-react/issues"
  },
  "dependencies": {
    "@xylabs/ts-scripts": "^1.0.22"
  },
  "description": "TypeScript project scripts for React",
  "devDependencies": {
    "@types/node": "^16.11.10",
    "@xylabs/eslint-config": "^2.1.4",
    "eslint": "^8",
    "prettier": "^2.5",
    "typescript": "^4"
  },
  "homepage": "https://xylabs.com",
  "keywords": [
    "xylabs",
    "react",
    "utility",
    "typescript"
  ],
  "license": "LGPL-3.0",
  "name": "@xylabs/ts-scripts-react",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xylabs/ts-scripts-react.git"
  },
  "scripts": {
    "build": "rm -r -f bin && yarn deps && yarn lint && yarn lint-pkg && yarn compile",
    "clean": "rm -f *.tsbuildinfo && rm -r -f ./bin",
    "compile": "tsc -p ./tsconfig.json",
    "cycle": "eslint --rule 'import/no-cycle: [1, { maxDepth: 10 }]' ./src",
    "dead": "yarn ts-prune",
    "deploy": "yarn build && yarn version --patch && yarn publish",
    "deps": "yarn depcheck",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "lint-pkg": "npmPkgJsonLint .",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install && yarn outdated",
    "up": "yarn ncu",
    "updo": "yarn install && yarn upgrade && yarn ncu -u && yarn reinstall"
  },
  "sideEffects": false,
  "version": "1.0.6"
}
